{
    "name": "baethon/laravel-resource",
    "description": "Conveniant factory for Laravel API resources",
    "keywords": ["laravel", "resource", "resources", "api", "helper", "http"],
    "type": "library",
    "require": {
        "illuminate/support": "7.*|8.*",
        "illuminate/http": "7.*|8.*"
    },
    "require-dev": {
        "orchestra/testbench": "^6.15",
        "phpunit/phpunit": "^9.5",
        "illuminate/database": "^8.33"
    },
    "autoload": {
        "psr-4": {
            "Baethon\\Laravel\\Resource\\": "src/"
        },
        "files": [
            "./src/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "App\\": "tests/stubs/",
            "Tests\\": "tests/"
        }
    },
    "license": "MIT",
    "authors": [
        {
            "name": "Radoslaw Mejer",
            "email": "radmen@baethon.com"
        }
    ],
    "extra": {
        "laravel": {
            "providers": [
                "Baethon\\Laravel\\Resource\\ServiceProvider"
            ]
        }
    }
}
